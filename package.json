  "bugs": {
    "url": "https://github.com/edu-tosel/axios-handler/issues"
  },
  "homepage": "https://github.com/edu-tosel/axios-handler#readme",
  "dependencies": {
    "axios": "^1.6.8"
  },
  "devDependencies": {
    "typescript": "^5.2.2",
    "rimraf": "^5.0.5",
    "@types/node": "^20.11.4"
  }
